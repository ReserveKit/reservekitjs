{
	"name": "reservekitjs",
	"private": false,
	"version": "0.0.1",
	"description": "ReserveKit JS SDK",
	"author": "ReserveKit Developer",
	"type": "module",
	"main": "./dist/cjs/index.cjs",
	"module": "./dist/es/index.js",
	"types": "./dist/types/index.d.ts",
	"typings": "./dist/types/index.d.ts",
	"exports": {
		".": {
			"require": "./dist/cjs/index.cjs",
			"import": "./dist/es/index.js",
			"default": "./dist/es/index.js",
			"types": "./dist/types/index.d.ts"
		}
	},
	"files": [
		"dist",
		"public",
		"README.md"
	],
	"scripts": {
		"clean": "rm -rf dist",
		"dev": "yarn build:es --watch",
		"build:cjs": "BUILD_TARGET=cjs vite build",
		"build:es": "BUILD_TARGET=es vite build",
		"build": "yarn clean && yarn build:cjs && yarn build:es",
		"test": "jest",
		"test:watch": "jest --watch",
		"test:coverage": "jest --coverage"
	},
	"license": "MIT",
	"repository": {
		"type": "git",
		"url": "git+https://github.com/qwerqy/reservekitjs.git"
	},
	"publishConfig": {
		"access": "public",
		"registry": "https://registry.npmjs.org"
	},
	"devDependencies": {
		"@types/jest": "^29.5.14",
		"@types/node": "^22.12.0",
		"jest": "^29.7.0",
		"ts-jest": "^29.2.5",
		"typescript": "~5.6.2",
		"vite": "^6.0.5",
		"vite-plugin-dts": "^4.5.0"
	}
}
