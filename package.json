{
	"name": "packagekit-js",
	"private": false,
	"version": "0.0.1",
	"description": "A Starter typescript/javaScript library for building packages",
	"author": "Qwerqy",
	"type": "module",
	"main": "./dist/cjs/index.cjs",
	"module": "./dist/es/index.js",
	"types": "./dist/types/index.d.ts",
	"typings": "./dist/types/index.d.ts",
	"exports": {
		".": {
			"require": "./dist/cjs/index.cjs",
			"import": "./dist/es/index.js",
			"default": "./dist/es/index.js",
			"types": "./dist/types/index.d.ts"
		}
	},
	"files": [
		"dist",
		"public",
		"README.md"
	],
	"scripts": {
		"clean": "rm -rf dist",
		"dev": "yarn build:es --watch",
		"build:cjs": "BUILD_TARGET=cjs vite build",
		"build:es": "BUILD_TARGET=es vite build",
		"build": "yarn clean && yarn build:cjs && yarn build:es"
	},
	"license": "MIT",
	"repository": {
		"type": "git",
		"url": "git+https://github.com/qwerqy/packagekit-js.git"
	},
	"publishConfig": {
		"access": "public",
		"registry": "https://registry.npmjs.org"
	},
	"devDependencies": {
		"@types/node": "^22.12.0",
		"typescript": "~5.6.2",
		"vite": "^6.0.5",
		"vite-plugin-dts": "^4.5.0"
	}
}
